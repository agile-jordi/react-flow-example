{
  "name": "react-flow-example",
  "version": "0.0.1",
  "description": "React Flow example",
  "repository": {
    "type": "git",
    "url": "git@github.com:agile-jordi/react-flow-example.git"
  },
  "//": [
    "Inspiration:",
    "- https://github.com/greypants/gulp-starter",
    "- https://www.npmjs.com/package/gulp-flowtype"
  ],
  "//browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "//browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "//browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "//devDependencies": {
    "browserify-shim": "^3.8.2",
    "coffeeify": "~0.7.0",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^1.0.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-sass": "~1.3.1",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.0.2",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0"
  },
  "devDependencies": {
    "browserify": "^8.0.2",
    "browser-sync": "~1.3.6",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-react": "*",
    "gulp-flowtype": "*",
    "gulp-util": "^3.0.0",
    "lodash": "^2.4.1",
    "reactify":"1.0.0",
    "require-dir": "^0.1.0",
    "pretty-hrtime": "~0.2.1",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "^2.2.1"
  },
  "dependencies": {
    "react": "~0.12.2"
  }
